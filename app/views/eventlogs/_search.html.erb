<h3>Search</h3><%= form_for @search, :url => eventlogs_path, :class => "form-horizontal", :html => {:method => :get} do |f| %>	<div class="form-inputs row">		<%= render :partial => 'shared/search/dropdown', :locals => { :f => f, :object => Event } %>			<%= render :partial => 'shared/search/dropdown', :locals => { :f => f, :object => Visitor } %>			<%= render :partial => 'shared/search/status', :locals => { :f => f } %>		</div>		<div class="form-inputs row">		<%= render :partial => 'shared/search/date', :locals => { :f => f } %>	</div>	</br>	<%= f.submit %><% end %>