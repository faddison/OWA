<h1>Listing visitors</h1>

<table>
  <tr>
    <th>First name</th>
    <th>Last name</th>
    <th>Language</th>
    <th>Email</th>
    <th>Phone</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @visitors.each do |visitor| %>
  <tr>
    <td><%= visitor.fname %></td>
    <td><%= visitor.lname %></td>
    <td><%= visitor.language %></td>
    <td><%= visitor.email %></td>
    <td><%= visitor.phone %></td>
    <td><%= link_to 'Show', visitor %></td>
    <td><%= link_to 'Edit', edit_visitor_path(visitor) %></td>
    <td><%= link_to 'Destroy', visitor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Visitor', new_visitor_path %>
