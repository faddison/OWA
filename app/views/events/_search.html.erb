<h3>Search</h3><%= form_for @search, :url => events_path, :class => "form-horizontal", :html => {:method => :get} do |f| %>	<div class="form-inputs row">		<%= render :partial => 'shared/search/like', :locals => { :f => f } %>		<%= render :partial => 'shared/search/dropdown', :locals => { :f => f, :object => Program } %>			<%= render :partial => 'shared/search/dropdown', :locals => { :f => f, :object => Facility } %>		<%= render :partial => 'shared/search/dropdown', :locals => { :f => f, :object => Eventtype } %>			<%= render :partial => 'shared/search/status', :locals => { :f => f } %>		</div>		<div class="form-inputs row">		<%= render :partial => 'shared/search/date', :locals => { :f => f } %>		<%= render :partial => 'shared/search/count', :locals => { :f => f, :attr_name => 'duration' } %>	</div>	</br>	<%= f.submit %><% end %>