
<h3>Search</h3>

<%= form_for @search, :url => brochurelogs_path, :html => {:method => :get} do |f| %>
<tr>
	<%= f.label :Brochure %>
	<%= f.collection_select :bname_contains, Brochure.all, :id, :name, :include_blank => true %>
	
	<%= f.label :Facility %>
	<%= f.collection_select :fname_contains, Facility.all, :id, :name, :include_blank => true %>
	
	<%= f.label :date %>
	<%= f.text_field :date_equals, :class => :datepicker  %>
	
	</br>
	<%= f.submit %>
<% end %>